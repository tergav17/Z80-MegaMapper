              		; Virtual mode should be off while we do this
0357: 3AD710  		ld	a,(zmm_ctrl_state)
035A: F5      		push	af
035B: CD9C02  		call	zmm_set_real
              		
              		; Do function call
035E: CD6D03  		call	0$
              		
              		; Reset bank 3
0361: 3ADB10  		ld	a,(zmm_bnk3_state)
0364: D333    		out	(zmm_bnk3),a
              		
              		; Restore register
0366: F1      		pop	af
0367: 32D710  		ld	(zmm_ctrl_state),a
036A: C38B02  		jp	zmm_ctrl_set
              		
              		; Allocate a new bank
036D: 1601    	0$:	ld	d,1
036F: CDB501  		call	mem_alloc
0372: 2A7211  		ld	hl,(res_bankmap)
0375: 77      		ld	(hl),a
0376: 23      		inc	hl
0377: 227211  		ld	(res_bankmap),hl
              		
              		; Set the bank in slot 3
037A: D333    		out	(zmm_bnk3),a
              		
              		; Set DMA address
037C: 11EF10  		ld	de,res_buffer
037F: 0E1A    		ld	c,bios_set_dma
0381: CD0500  		call	bdos
              		
              		; Set pointer	
0384: 2100C0  		ld	hl,zmm_top
0387: 227411  		ld	(res_pointer),hl
              		
