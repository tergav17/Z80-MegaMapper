              	; Initalize resources
              	;
              	; Returns nothing
              	; Uses: AF, BC, HL
0341:         	res_init:
              		; Start by zero-terminating string
0341: 218000  		ld	hl,cpm_command
0344: 4E      		ld	c,(hl)
0345: 0600    		ld	b,0
