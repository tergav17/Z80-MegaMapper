03A0: 21EF10  		ld	hl,res_buffer
03A3: ED5B7411		ld	de,(res_pointer)
03A7: 018000  		ld	bc,128
03AA: EDB0    		ldir
03AC: ED537411		ld	(res_pointer),de
              		
              		; Back to real mode
03B0: CD9C02  		call	zmm_set_real 
              		
