0348: 23      		inc	hl
0349: 70      		ld	(hl),b
              		
034A: C9      		ret
              		
              	; Loads an open resource into a bankmap
              	; Bankmap will be zero-padded to fill requested number of records
              	; HL = Address of bankmap
              	; BC = Number of records (128 byte blocks) to load
              	;
              	; Returns nothing
              	; Uses: all
034B:         	res_load:
              		; Set read flag
034B: 3EFF    		ld	a,0xFF
034D: 325F11  		ld	(res_do_read),a
              	
              		; Save args
0350: 226011  		ld	(res_bankmap),hl
0353: ED435D11		ld	(res_sectors),bc
              	
              		; Virtual mode should be off while we do this
0357: 3AC510  		ld	a,(zmm_ctrl_state)
035A: F5      		push	af
035B: CD9C02  		call	zmm_set_real
              		
              		; Do function call
035E: CD6D03  		call	0$
              		
              		; Reset bank 3
0361: 3AC910  		ld	a,(zmm_bnk3_state)
0364: D333    		out	(zmm_bnk3),a
              		
              		; Restore register
0366: F1      		pop	af
0367: 32C510  		ld	(zmm_ctrl_state),a
036A: C38B02  		jp	zmm_ctrl_set
              		
              		; Allocate a new bank
036D: 1601    	0$:	ld	d,1
036F: CDB501  		call	mem_alloc
0372: 2A6011  		ld	hl,(res_bankmap)
0375: 77      		ld	(hl),a
0376: 23      		inc	hl
0377: 226011  		ld	(res_bankmap),hl
              		
              		; Set the bank in slot 3
037A: D333    		out	(zmm_bnk3),a
              		
              		; Set DMA address
037C: 11DD10  		ld	de,res_buffer
037F: 0E1A    		ld	c,bios_set_dma
0381: CD0500  		call	bdos
              		
              		; Set pointer	
0384: 2100C0  		ld	hl,zmm_top
0387: 226211  		ld	(res_pointer),hl
              		
              		; We will be loading up to 128 records at a time here
038A: 0680    		ld	b,128
              		
              		; Do read operation
038C: C5      	1$:	push	bc
038D: 116411  		ld	de,res_fcb
0390: 0E14    		ld	c,bdos_read
0392: 3A5F11  		ld	a,(res_do_read)
0395: B7      		or	a
0396: C40500  		call	nz,bdos
              		
              		; Check file I/O result
0399: B7      		or	a
039A: C4C303  		call	nz,50$
              		
