04AC: 13      		inc	de
04AD: 23      		inc	hl
04AE: 10F1    		djnz	6$
04B0: C3BA04  		jp	8$
              		
              		; Fill remains of FCB file extension
04B3: 3E3F    	7$:	ld	a,'?'
04B5: 12      		ld	(de),a
04B6: 13      		inc	de
04B7: 10D1    		djnz	4$
04B9: 23      		inc	hl
              	
              		; We should get a zero
04BA: 7E      	8$:	ld 	a,(hl)
