045F: 3ADC10  		ld	a,(res_argument)
0462: D640    		sub	'A'-1
0464: FE11    		cp	17
0466: D2C904  		jp	nc,99$
0469: 327611  		ld	(res_fcb_drive),a
046C: 23      		inc	hl
046D: 23      		inc	hl
              	
