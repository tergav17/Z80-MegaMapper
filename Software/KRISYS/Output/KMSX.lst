03BE: 10CC    		djnz	1$
03C0: C36D03  		jp	0$
              	
              		; Zero buffer and reset read flag
03C3: AF      	50$:	xor	a
03C4: 325F11  		ld	(res_do_read),a
              		
03C7: 21DD10  		ld	hl,res_buffer
03CA: 11DE10  		ld	de,res_buffer+1
