049E: 116D11  		ld	de,res_fcb_type
              		
              		; Copy it over
04A1: 7E      	6$:	ld	a,(hl)
04A2: B7      		or	a
04A3: CABA04  		jp	z,8$
04A6: FE2A    		cp	'*'
04A8: CAB304  		jp	z,7$
