046E: 0608    	2$:	ld	b,8
0470: 117711  		ld	de,res_fcb_name
              		
              		; Copy it over
0473: 7E      	3$:	ld	a,(hl)
0474: B7      		or	a
0475: CAC904  		jp	z,99$
0478: FE2A    		cp	'*'
047A: CA8A04  		jp	z,4$
047D: FE2E    		cp	'.'
047F: CA9104  		jp	z,5$
0482: 12      		ld	(de),a
0483: 13      		inc	de
0484: 23      		inc	hl
0485: 10EC    		djnz	3$
0487: C39104  		jp	5$
              	
