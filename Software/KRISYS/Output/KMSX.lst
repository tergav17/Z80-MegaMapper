039D: CD9102  		call	zmm_set_virt
              		
              		; Copy record to bank
03A0: 21DD10  		ld	hl,res_buffer
03A3: ED5B6211		ld	de,(res_pointer)
03A7: 018000  		ld	bc,128
03AA: EDB0    		ldir
03AC: ED536211		ld	(res_pointer),de
              		
              		; Back to real mode
03B0: CD9C02  		call	zmm_set_real 
              		
              		; Get BC back to restore sector counter
03B3: C1      		pop	bc
              		
