038C: C5      	1$:	push	bc
038D: 117611  		ld	de,res_fcb
0390: 0E14    		ld	c,bdos_read
0392: 3A7111  		ld	a,(res_do_read)
0395: B7      		or	a
0396: C40500  		call	nz,bdos
              		
              		; Check file I/O result
0399: B7      		or	a
039A: C4C303  		call	nz,50$
              		
