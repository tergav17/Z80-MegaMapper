0439: AF      	1$:	xor	a
043A: 216411  		ld	hl,res_fcb
043D: 116511  		ld	de,res_fcb+1
0440: 012300  		ld	bc ,36-1
0443: 77      		ld	(hl),a
0444: EDB0    		ldir
              		
0446: 3E20    		ld	a,0x20
0448: 216511  		ld	hl,res_fcb_name
044B: 116611  		ld	de,res_fcb_name+1
044E: 010A00  		ld	bc ,11-1
0451: 77      		ld	(hl),a
0452: EDB0    		ldir
              	
              		; Is there a prefix?
0454: 21CA10  		ld	hl,res_argument
0457: 3ACB10  		ld	a,(res_argument+1)
045A: FE3A    		cp	':'
045C: C26E04  		jp	nz,2$
              		
              		; Set prefix
045F: 3ACA10  		ld	a,(res_argument)
0462: D640    		sub	'A'-1
0464: FE11    		cp	17
0466: D2C904  		jp	nc,99$
0469: 326411  		ld	(res_fcb_drive),a
046C: 23      		inc	hl
046D: 23      		inc	hl
              	
              		; HL = Proper filename start
046E: 0608    	2$:	ld	b,8
0470: 116511  		ld	de,res_fcb_name
              		
              		; Copy it over
0473: 7E      	3$:	ld	a,(hl)
0474: B7      		or	a
0475: CAC904  		jp	z,99$
0478: FE2A    		cp	'*'
047A: CA8A04  		jp	z,4$
047D: FE2E    		cp	'.'
047F: CA9104  		jp	z,5$
0482: 12      		ld	(de),a
0483: 13      		inc	de
0484: 23      		inc	hl
0485: 10EC    		djnz	3$
0487: C39104  		jp	5$
              	
              		; Fill remains of FCB file name
048A: 3E3F    	4$:	ld	a,'?'
048C: 12      		ld	(de),a
048D: 13      		inc	de
048E: 10FA    		djnz	4$
0490: 23      		inc	hl
              	
              		; We should either see a '.' or a null character
0491: 7E      	5$:	ld	a,(hl)
0492: B7      		or	a
0493: CABA04  		jp	z,8$
0496: FE2E    		cp	'.'
0498: C2C904  		jp	nz,99$
049B: 23      		inc	hl
              		
              		; Fill in extension
