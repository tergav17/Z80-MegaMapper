03B4: 2A5D11  		ld	hl,(res_sectors)
03B7: 2B      		dec	hl
03B8: 225D11  		ld	(res_sectors),hl
03BB: 7C      		ld	a,h
03BC: B5      		or	l
03BD: C8      		ret	z
              		
