              		; Print error message
034B: 0E09    		ld	c,bdos_print
034D: 111309  		ld	de,str_missing
0350: CD0500  		call	bdos
              		
              		; Print resource name
0353: ED5BD10C		ld	de,(res_current)
0357: CD4804  		call	res_printzt
              		
              		; CRLF
035A: 0E09    		ld	c,bdos_print
035C: 11370A  		ld	de,str_crlf
035F: CD0500  		call	bdos
              		
              		; Exit
0362: C35C08  		jp	cpm_exit
              		
              	; Opens a file based on the resource argument
              	; If the file cannot be opened, an error will be thrown
              	; (res_argument) = File to open
              	;
              	; Returns nothing
              	; Uses: all
0365:         	res_open:
              		; Virtual mode should be off while we do this
0365: 3ABB0C  		ld	a,(zmm_ctrl_state)
0368: F5      		push	af
0369: CD1002  		call	zmm_set_real
              		
              		; Do function call
036C: CD7603  		call	0$
              		
              		; Restore register
036F: F1      		pop	af
0370: 32BB0C  		ld	(zmm_ctrl_state),a
0373: C3FF01  		jp	zmm_ctrl_set
              		
              		; Let the user know we are loading stuff
0376: 0E09    	0$:	ld	c,bdos_print
0378: 11D608  		ld	de,str_load_a
037B: CD0500  		call	bdos
              		
              		; Print resource name
037E: ED5BD10C		ld	de,(res_current)
0382: CD4804  		call	res_printzt
              		
              		; Next string
0385: 0E09    		ld	c,bdos_print
0387: 11DF08  		ld	de,str_load_b
038A: CD0500  		call	bdos
              		
              		; Print file name
038D: 11C00C  		ld	de,res_argument
0390: CD4804  		call	res_printzt
              		
              		; CRLF
0393: 0E09    		ld	c,bdos_print
0395: 11370A  		ld	de,str_crlf
0398: CD0500  		call	bdos
              		
              		; Detect if there is an argument
039B: 3AC00C  		ld	a,(res_argument)
039E: B7      		or	a
039F: C2AD03  		jp	nz,1$
              		
              		; No argument, error!
03A2: 0E09    		ld	c,bdos_print
03A4: 11E608  		ld	de,str_arg_empty
03A7: CD0500  		call	bdos
03AA: C35C08  		jp	cpm_exit
              	
              		; Reset fields
03AD: AF      	1$:	xor	a
03AE: 215A0D  		ld	hl,res_fcb
03B1: 115B0D  		ld	de,res_fcb+1
03B4: 012300  		ld	bc ,36-1
03B7: 77      		ld	(hl),a
03B8: EDB0    		ldir
              		
03BA: 3E20    		ld	a,0x20
03BC: 215B0D  		ld	hl,res_fcb_name
03BF: 115C0D  		ld	de,res_fcb_name+1
03C2: 010A00  		ld	bc ,11-1
03C5: 77      		ld	(hl),a
03C6: EDB0    		ldir
              	
              		; Is there a prefix?
03C8: 21C00C  		ld	hl,res_argument
03CB: 3AC10C  		ld	a,(res_argument+1)
03CE: FE3A    		cp	':'
03D0: C2E203  		jp	nz,2$
              		
              		; Set prefix
03D3: 3AC00C  		ld	a,(res_argument)
03D6: D640    		sub	'A'-1
03D8: FE11    		cp	17
03DA: D23D04  		jp	nc,99$
03DD: 325A0D  		ld	(res_fcb_drive),a
03E0: 23      		inc	hl
03E1: 23      		inc	hl
              	
              		; HL = Proper filename start
03E2: 0608    	2$:	ld	b,8
03E4: 115B0D  		ld	de,res_fcb_name
              		
              		; Copy it over
03E7: 7E      	3$:	ld	a,(hl)
03E8: B7      		or	a
03E9: CA3D04  		jp	z,99$
03EC: FE2A    		cp	'*'
03EE: CAFE03  		jp	z,4$
03F1: FE2E    		cp	'.'
03F3: CA0504  		jp	z,5$
03F6: 12      		ld	(de),a
03F7: 13      		inc	de
03F8: 23      		inc	hl
03F9: 10EC    		djnz	3$
03FB: C30504  		jp	5$
              	
              		; Fill remains of FCB file name
03FE: 3E3F    	4$:	ld	a,'?'
0400: 12      		ld	(de),a
0401: 13      		inc	de
0402: 10FA    		djnz	4$
0404: 23      		inc	hl
              	
              		; We should either see a '.' or a null character
0405: 7E      	5$:	ld	a,(hl)
0406: B7      		or	a
0407: CA2E04  		jp	z,8$
040A: FE2E    		cp	'.'
040C: C23D04  		jp	nz,99$
040F: 23      		inc	hl
              		
              		; Fill in extension
0410: 0603    		ld	b,3
0412: 11630D  		ld	de,res_fcb_type
              		
              		; Copy it over
0415: 7E      	6$:	ld	a,(hl)
0416: B7      		or	a
0417: CA2E04  		jp	z,8$
041A: FE2A    		cp	'*'
041C: CA2704  		jp	z,7$
041F: 12      		ld	(de),a
0420: 13      		inc	de
0421: 23      		inc	hl
0422: 10F1    		djnz	6$
0424: C32E04  		jp	8$
              		
              		; Fill remains of FCB file extension
0427: 3E3F    	7$:	ld	a,'?'
0429: 12      		ld	(de),a
042A: 13      		inc	de
042B: 10D1    		djnz	4$
042D: 23      		inc	hl
              	
              		; We should get a zero
042E: 7E      	8$:	ld 	a,(hl)
042F: B7      		or	a
0430: C23D04  		jp	nz,99$
              		
              		; It is filled in, attempt to open
0433: 0E0F    		ld	c,bdos_open
0435: 115A0D  		ld	de,res_fcb
0438: CD0500  		call	bdos
              		
              		; Check error
043B: 3C      		inc	a
043C: C0      		ret	nz
              		
              		; Error!
043D: 0E09    	99$:	ld	c,bdos_print
043F: 11FD08  		ld	de,str_arg_fail
0442: CD0500  		call	bdos
0445: C35C08  		jp	cpm_exit
              		
              		
              	; Print a zero terminated string
              	; We should be in real mode for this
              	; DE = String
