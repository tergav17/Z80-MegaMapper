018E: BA      		cp	d
018F: C0      		ret	nz
              		
              		; Free bank
0190: 3EFF    		ld	a,0xFF
0192: 77      		ld	(hl),a
              		
              		; Increment banks free
0193: 212303  		ld	hl,banks_free
0196: 34      		inc	(hl)
0197: C9      		ret
              		
