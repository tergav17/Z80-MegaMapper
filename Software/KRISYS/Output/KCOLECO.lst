04CD: B7      		or	a
04CE: F2AC06  		jp	p,trap_continue
              		
              		; OK, a trap did occur.
              		; Are we doing "classic" I/O or extended I/O?
04D1:         	trap_io:	
04D1: FEE8    		cp	0b11101000
04D3: DAED04  		jp	c,trap_io_ext
              		
