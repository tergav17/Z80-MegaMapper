              	.area	_TEXT
              	
              	; Initalize resources
              	;
              	; Returns nothing
              	; Uses: AF, BC, HL
02B8:         	res_init:
              		; Start by zero-terminating string
02B8: 218000  		ld	hl,cpm_command
02BB: 4E      		ld	c,(hl)
02BC: 0600    		ld	b,0
02BE: 09      		add	hl,bc
02BF: 23      		inc	hl
02C0: 70      		ld	(hl),b
              		
02C1: C9      		ret
              		
              	; Loads an open resource into a bankmap
              	; Bankmap will be zero-padded to fill requested number of records
              	; HL = Address of bankmap
              	; BC = Number of records (128 byte blocks) to load
              	;
              	; Returns nothing
              	; Uses: all
02C2:         	res_load:
              		; Set read flag
02C2: 3EFF    		ld	a,0xFF
02C4: 328A0D  		ld	(res_do_read),a
              	
              		; Save args
02C7: 228B0D  		ld	(res_bankmap),hl
02CA: ED43880D		ld	(res_sectors),bc
              	
              		; Virtual mode should be off while we do this
02CE: 3AF00C  		ld	a,(zmm_ctrl_state)
02D1: F5      		push	af
02D2: CD1302  		call	zmm_set_real
              		
              		; Do function call
02D5: CDE402  		call	0$
              		
              		; Reset bank 3
02D8: 3AF40C  		ld	a,(zmm_bnk3_state)
02DB: D333    		out	(zmm_bnk3),a
              		
              		; Restore register
02DD: F1      		pop	af
02DE: 32F00C  		ld	(zmm_ctrl_state),a
02E1: C30202  		jp	zmm_ctrl_set
              		
              		; Allocate a new bank
02E4: 1601    	0$:	ld	d,1
02E6: CD6501  		call	mem_alloc
02E9: 2A8B0D  		ld	hl,(res_bankmap)
02EC: 77      		ld	(hl),a
02ED: 23      		inc	hl
02EE: 228B0D  		ld	(res_bankmap),hl
              		
              		; Set the bank in slot 3
02F1: D333    		out	(zmm_bnk3),a
              		
              		; Set DMA address
02F3: 11080D  		ld	de,res_buffer
02F6: 0E1A    		ld	c,bios_set_dma
02F8: CD0500  		call	bdos
              		
              		; Set pointer	
02FB: 2100C0  		ld	hl,zmm_top
02FE: 228D0D  		ld	(res_pointer),hl
              		
              		; We will be loading up to 128 records at a time here
0301: 0680    		ld	b,128
              		
              		; Do read operation
0303: C5      	1$:	push	bc
0304: 118F0D  		ld	de,res_fcb
0307: 0E14    		ld	c,bdos_read
0309: 3A8A0D  		ld	a,(res_do_read)
030C: B7      		or	a
030D: C40500  		call	nz,bdos
              		
              		; Check file I/O result
0310: B7      		or	a
0311: C43A03  		call	nz,50$
              		
              		; Go to virtual mode
0314: CD0802  		call	zmm_set_virt
              		
              		; Copy record to bank
0317: 21080D  		ld	hl,res_buffer
031A: ED5B8D0D		ld	de,(res_pointer)
031E: 018000  		ld	bc,128
0321: EDB0    		ldir
0323: ED538D0D		ld	(res_pointer),de
              		
              		; Back to real mode
0327: CD1302  		call	zmm_set_real 
              		
              		; Get BC back to restore sector counter
032A: C1      		pop	bc
              		
              		; Decrement record count
032B: 2A880D  		ld	hl,(res_sectors)
032E: 2B      		dec	hl
032F: 22880D  		ld	(res_sectors),hl
0332: 7C      		ld	a,h
0333: B5      		or	l
0334: C8      		ret	z
              		
              		; Go get another sector
0335: 10CC    		djnz	1$
0337: C3E402  		jp	0$
              	
              		; Zero buffer and reset read flag
033A: AF      	50$:	xor	a
033B: 328A0D  		ld	(res_do_read),a
              		
033E: 21080D  		ld	hl,res_buffer
0341: 11090D  		ld	de,res_buffer+1
0344: 017F00  		ld	bc,128-1
0347: 77      		ld	(hl),a
0348: EDB0    		ldir
              	
034A: C9      		ret
              		
              	; Throws an error a missing resource
              	; Resource name will be last attempted to locate
              	;
              	; Does not return
              	; Uses: N/A
034B:         	res_missing:
034B: CD1302  		call	zmm_set_real
              		
              		; Print error message
034E: 0E09    		ld	c,bdos_print
0350: 114A09  		ld	de,str_missing
0353: CD0500  		call	bdos
              		
              		; Print resource name
0356: ED5B060D		ld	de,(res_current)
035A: CD4B04  		call	res_printzt
              		
              		; CRLF
035D: 0E09    		ld	c,bdos_print
035F: 116C0A  		ld	de,str_crlf
0362: CD0500  		call	bdos
              		
              		; Exit
0365: C39308  		jp	cpm_exit
              		
              	; Opens a file based on the resource argument
              	; If the file cannot be opened, an error will be thrown
              	; (res_argument) = File to open
              	;
              	; Returns nothing
              	; Uses: all
0368:         	res_open:
              		; Virtual mode should be off while we do this
0368: 3AF00C  		ld	a,(zmm_ctrl_state)
036B: F5      		push	af
036C: CD1302  		call	zmm_set_real
              		
              		; Do function call
036F: CD7903  		call	0$
              		
              		; Restore register
0372: F1      		pop	af
0373: 32F00C  		ld	(zmm_ctrl_state),a
0376: C30202  		jp	zmm_ctrl_set
              		
              		; Let the user know we are loading stuff
0379: 0E09    	0$:	ld	c,bdos_print
037B: 110D09  		ld	de,str_load_a
037E: CD0500  		call	bdos
              		
              		; Print resource name
0381: ED5B060D		ld	de,(res_current)
0385: CD4B04  		call	res_printzt
              		
              		; Next string
0388: 0E09    		ld	c,bdos_print
038A: 111609  		ld	de,str_load_b
038D: CD0500  		call	bdos
              		
              		; Print file name
0390: 11F50C  		ld	de,res_argument
0393: CD4B04  		call	res_printzt
              		
              		; CRLF
0396: 0E09    		ld	c,bdos_print
0398: 116C0A  		ld	de,str_crlf
039B: CD0500  		call	bdos
              		
              		; Detect if there is an argument
039E: 3AF50C  		ld	a,(res_argument)
03A1: B7      		or	a
03A2: C2B003  		jp	nz,1$
              		
              		; No argument, error!
03A5: 0E09    		ld	c,bdos_print
03A7: 111D09  		ld	de,str_arg_empty
03AA: CD0500  		call	bdos
03AD: C39308  		jp	cpm_exit
              	
              		; Reset fields
03B0: AF      	1$:	xor	a
03B1: 218F0D  		ld	hl,res_fcb
03B4: 11900D  		ld	de,res_fcb+1
03B7: 012300  		ld	bc ,36-1
03BA: 77      		ld	(hl),a
03BB: EDB0    		ldir
              		
03BD: 3E20    		ld	a,0x20
03BF: 21900D  		ld	hl,res_fcb_name
03C2: 11910D  		ld	de,res_fcb_name+1
03C5: 010A00  		ld	bc ,11-1
03C8: 77      		ld	(hl),a
03C9: EDB0    		ldir
              	
              		; Is there a prefix?
03CB: 21F50C  		ld	hl,res_argument
03CE: 3AF60C  		ld	a,(res_argument+1)
03D1: FE3A    		cp	':'
03D3: C2E503  		jp	nz,2$
              		
              		; Set prefix
03D6: 3AF50C  		ld	a,(res_argument)
03D9: D640    		sub	'A'-1
03DB: FE11    		cp	17
03DD: D24004  		jp	nc,99$
03E0: 328F0D  		ld	(res_fcb_drive),a
03E3: 23      		inc	hl
03E4: 23      		inc	hl
              	
              		; HL = Proper filename start
03E5: 0608    	2$:	ld	b,8
03E7: 11900D  		ld	de,res_fcb_name
              		
              		; Copy it over
03EA: 7E      	3$:	ld	a,(hl)
03EB: B7      		or	a
03EC: CA4004  		jp	z,99$
03EF: FE2A    		cp	'*'
03F1: CA0104  		jp	z,4$
03F4: FE2E    		cp	'.'
03F6: CA0804  		jp	z,5$
03F9: 12      		ld	(de),a
03FA: 13      		inc	de
03FB: 23      		inc	hl
03FC: 10EC    		djnz	3$
03FE: C30804  		jp	5$
              	
              		; Fill remains of FCB file name
0401: 3E3F    	4$:	ld	a,'?'
0403: 12      		ld	(de),a
0404: 13      		inc	de
0405: 10FA    		djnz	4$
0407: 23      		inc	hl
              	
              		; We should either see a '.' or a null character
0408: 7E      	5$:	ld	a,(hl)
0409: B7      		or	a
040A: CA3104  		jp	z,8$
040D: FE2E    		cp	'.'
040F: C24004  		jp	nz,99$
0412: 23      		inc	hl
              		
              		; Fill in extension
0413: 0603    		ld	b,3
0415: 11980D  		ld	de,res_fcb_type
              		
              		; Copy it over
0418: 7E      	6$:	ld	a,(hl)
0419: B7      		or	a
041A: CA3104  		jp	z,8$
041D: FE2A    		cp	'*'
041F: CA2A04  		jp	z,7$
0422: 12      		ld	(de),a
0423: 13      		inc	de
0424: 23      		inc	hl
0425: 10F1    		djnz	6$
0427: C33104  		jp	8$
              		
              		; Fill remains of FCB file extension
042A: 3E3F    	7$:	ld	a,'?'
042C: 12      		ld	(de),a
042D: 13      		inc	de
042E: 10D1    		djnz	4$
0430: 23      		inc	hl
              	
              		; We should get a zero
0431: 7E      	8$:	ld 	a,(hl)
0432: B7      		or	a
0433: C24004  		jp	nz,99$
              		
              		; It is filled in, attempt to open
0436: 0E0F    		ld	c,bdos_open
0438: 118F0D  		ld	de,res_fcb
043B: CD0500  		call	bdos
              		
              		; Check error
043E: 3C      		inc	a
043F: C0      		ret	nz
              		
              		; Error!
0440: 0E09    	99$:	ld	c,bdos_print
0442: 113409  		ld	de,str_arg_fail
0445: CD0500  		call	bdos
0448: C39308  		jp	cpm_exit
              		
              		
              	; Print a zero terminated string
              	; We should be in real mode for this
              	; DE = String
              	;
              	; Returns nothing
              	; Uses: All
044B:         	res_printzt:
044B: 1A      	0$:	ld	a,(de)
044C: B7      		or	a
044D: C8      		ret	z
              		
              		; Print character
044E: D5      		push	de
044F: 5F      		ld	e,a
0450: 0E02    		ld	c,bdos_con_out
0452: CD0500  		call	bdos
0455: D1      		pop	de
0456: 13      		inc	de
0457: C34B04  		jp	0$
              	
              	; Find a resource from the command line
              	; If the resource is found, the contents will be cached in memory
              	; DE = Name of resource (upper case only) 
              	;
              	; Returns A = 0xFF if no resource is found
              	; Uses: AF, BC, DE, HL
045A:         	res_locate:
              		; Save resource
045A: ED53060D		ld	(res_current),de
              	
              		; Travel to the start of arguments
045E: 218100  		ld	hl,cpm_command+1
0461: 7E      	0$:	ld	a,(hl)
0462: B7      		or	a
0463: CAC004  		jp	z,99$
0466: FE21    		cp	0x21
0468: D26F04  		jp	nc,1$
046B: 23      		inc	hl
046C: C36104  		jp	0$
              		
              		; Found an argument
              		; Check it against the contents of (DE)
              		; Also must start with '-'
046F: FE2D    	1$:	cp	'-'
0471: C28204  		jp	nz,3$
0474: 23      		inc	hl
0475: D5      		push	de
0476: 1A      	2$:	ld	a,(de)
              		
              		; Check if at end of string
0477: B7      		or	a
0478: CA9004  		jp	z,4$
              		
              		; No? Well lets see if (de) = (hl)
047B: BE      		cp	(hl)
047C: 23      		inc	hl
047D: 13      		inc	de
047E: CA7604  		jp	z,2$
              		
              		; Strings are different!
              		; Escape from the current argument and continue
0481: D1      		pop	de
0482: 7E      	3$:	ld	a,(hl)
0483: B7      		or	a
0484: CAC004  		jp	z,99$
0487: FE21    		cp	0x21
0489: DA6104  		jp	c,0$
048C: 23      		inc	hl
048D: C38204  		jp	3$
              	
              		; Make sure we are at the end of the argument as well
0490: D1      	4$: 	pop	de
0491: 7E      		ld	a,(hl)
0492: FE21    		cp	0x21
0494: D28204  		jp	nc,3$
              		
              		; Ok, lets copy the argument into memory if it exists
0497: 11F50C  		ld	de,res_argument
049A: AF      		xor	a
049B: 12      		ld	(de),a
              		
              		; Travel to the start of the argument
049C: 7E      	5$:	ld	a,(hl)
049D: B7      		or	a
049E: CABE04  		jp	z,89$
04A1: FE21    		cp	0x21
04A3: D2AA04  		jp	nc,6$
04A6: 23      		inc	hl
04A7: C39C04  		jp	5$
              	
              		; Make sure it doesn't start with '-'
04AA: FE2D    	6$:	cp	'-'
04AC: CABE04  		jp	z,89$
              		
              		; Ok, lets copy up 16 bytes of this
04AF: 0610    		ld	b,arg_size
04B1: 7E      	7$:	ld	a,(hl)
04B2: FE21    		cp	0x21
04B4: DABC04  		jp	c,8$
04B7: 12      		ld	(de),a
04B8: 23      		inc	hl
04B9: 13      		inc	de
04BA: 10F5    		djnz	7$
              	
              		; Zero terminate
04BC: AF      	8$:	xor	a
04BD: 12      		ld	(de),a
              		
              		; Good ending
04BE: AF      	89$:	xor	a
04BF: C9      		ret
              	
              		; Bad ending
04C0: 3EFF    	99$:	ld	a,0xFF
04C2: C9      		ret
              		
