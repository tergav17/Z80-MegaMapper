031C: 1601    	0$:	ld	d,1
031E: CD9D01  		call	mem_alloc
0321: 2A150F  		ld	hl,(res_bankmap)
0324: 77      		ld	(hl),a
0325: 23      		inc	hl
0326: 22150F  		ld	(res_bankmap),hl
              		
              		; Set the bank in slot 3
0329: D333    		out	(zmm_bnk3),a
              		
              		; Set DMA address
032B: 11920E  		ld	de,res_buffer
032E: 0E1A    		ld	c,bios_set_dma
0330: CD0500  		call	bdos
              		
              		; Set pointer	
