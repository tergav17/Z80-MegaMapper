04D0: B7      		or	a
04D1: F2AC06  		jp	p,trap_continue
              		
              		; OK, a trap did occur.
              		; Are we doing "classic" I/O or extended I/O?
04D4:         	trap_io:	
04D4: FEE8    		cp	0b11101000
04D6: DAF004  		jp	c,trap_io_ext
              		
              		; In or out?
04D9: FEEC    		cp	0b11101100
04DB: DAE704  		jp	c,0$
              		
