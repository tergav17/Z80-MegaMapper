0336: 22170F  		ld	(res_pointer),hl
              		
              		; We will be loading up to 128 records at a time here
0339: 0680    		ld	b,128
              		
              		; Do read operation
033B: C5      	1$:	push	bc
033C: 11190F  		ld	de,res_fcb
033F: 0E14    		ld	c,bdos_read
0341: 3A140F  		ld	a,(res_do_read)
0344: B7      		or	a
0345: C40500  		call	nz,bdos
              		
