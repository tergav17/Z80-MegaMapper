0B99: FE80    		cp	0x80
0B9B: CAB00B  		jp	z,20$
              		
              		; Check for joystick 2
0B9E: FE81    		cp	0x81
0BA0: CAB50B  		jp	z,21$
              		
              		; Check for momentary keys
0BA3: FEE0    		cp	0xE0
0BA5: D2BB0B  		jp	nc,30$
              		
              		; Joystick data byte?
0BA8: E6E0    		and	0b11100000
0BAA: FEA0    		cp	0b10100000
0BAC: CABC0B  		jp	z,40$
              		
