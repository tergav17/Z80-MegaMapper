02BE: 09      		add	hl,bc
02BF: 23      		inc	hl
02C0: 70      		ld	(hl),b
              		
02C1: C9      		ret
              		
              	; Loads an open resource into a bankmap
              	; Bankmap will be zero-padded to fill requested number of records
              	; HL = Address of bankmap
              	; BC = Number of records (128 byte blocks) to load
              	;
              	; Returns nothing
              	; Uses: all
02C2:         	res_load:
              		; Set read flag
02C2: 3EFF    		ld	a,0xFF
02C4: 32810E  		ld	(res_do_read),a
              	
              		; Save args
02C7: 22820E  		ld	(res_bankmap),hl
02CA: ED437F0E		ld	(res_sectors),bc
              	
              		; Virtual mode should be off while we do this
02CE: 3AE70D  		ld	a,(zmm_ctrl_state)
02D1: F5      		push	af
02D2: CD1302  		call	zmm_set_real
              		
              		; Do function call
02D5: CDE402  		call	0$
              		
              		; Reset bank 3
02D8: 3AEB0D  		ld	a,(zmm_bnk3_state)
02DB: D333    		out	(zmm_bnk3),a
              		
              		; Restore register
02DD: F1      		pop	af
02DE: 32E70D  		ld	(zmm_ctrl_state),a
02E1: C30202  		jp	zmm_ctrl_set
              		
              		; Allocate a new bank
02E4: 1601    	0$:	ld	d,1
02E6: CD6501  		call	mem_alloc
02E9: 2A820E  		ld	hl,(res_bankmap)
02EC: 77      		ld	(hl),a
02ED: 23      		inc	hl
02EE: 22820E  		ld	(res_bankmap),hl
              		
              		; Set the bank in slot 3
02F1: D333    		out	(zmm_bnk3),a
              		
              		; Set DMA address
02F3: 11FF0D  		ld	de,res_buffer
02F6: 0E1A    		ld	c,bios_set_dma
02F8: CD0500  		call	bdos
              		
              		; Set pointer	
02FB: 2100C0  		ld	hl,zmm_top
02FE: 22840E  		ld	(res_pointer),hl
              		
              		; We will be loading up to 128 records at a time here
0301: 0680    		ld	b,128
              		
              		; Do read operation
0303: C5      	1$:	push	bc
0304: 11860E  		ld	de,res_fcb
0307: 0E14    		ld	c,bdos_read
0309: 3A810E  		ld	a,(res_do_read)
030C: B7      		or	a
030D: C40500  		call	nz,bdos
              		
              		; Check file I/O result
0310: B7      		or	a
0311: C43A03  		call	nz,50$
              		
              		; Go to virtual mode
0314: CD0802  		call	zmm_set_virt
              		
              		; Copy record to bank
0317: 21FF0D  		ld	hl,res_buffer
031A: ED5B840E		ld	de,(res_pointer)
031E: 018000  		ld	bc,128
0321: EDB0    		ldir
0323: ED53840E		ld	(res_pointer),de
              		
              		; Back to real mode
0327: CD1302  		call	zmm_set_real 
              		
              		; Get BC back to restore sector counter
032A: C1      		pop	bc
              		
              		; Decrement record count
032B: 2A7F0E  		ld	hl,(res_sectors)
032E: 2B      		dec	hl
032F: 227F0E  		ld	(res_sectors),hl
0332: 7C      		ld	a,h
0333: B5      		or	l
0334: C8      		ret	z
              		
              		; Go get another sector
0335: 10CC    		djnz	1$
0337: C3E402  		jp	0$
              	
              		; Zero buffer and reset read flag
033A: AF      	50$:	xor	a
033B: 32810E  		ld	(res_do_read),a
              		
033E: 21FF0D  		ld	hl,res_buffer
0341: 11000E  		ld	de,res_buffer+1
