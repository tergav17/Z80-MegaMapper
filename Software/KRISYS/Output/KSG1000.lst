0363: 2A120F  		ld	hl,(res_sectors)
0366: 2B      		dec	hl
0367: 22120F  		ld	(res_sectors),hl
036A: 7C      		ld	a,h
036B: B5      		or	l
036C: C8      		ret	z
              		
