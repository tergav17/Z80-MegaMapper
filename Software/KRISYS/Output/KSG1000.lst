04E4: C3AC06  		jp	trap_continue
              		
              		; Out it is
04E7: 3AFF6F  	0$:	ld	a,(trap_a_value)
04EA: CD0209  		call	out_handle
04ED: C3AC06  		jp	trap_continue
              	
              	
