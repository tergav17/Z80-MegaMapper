              	; Handle a "joystick" event
0B8A:         	sg_joystick:
              		
              		; Get the latest scancode from the keyboard
0B8A: DB90    		in	a,(nabu_key_data)
0B8C: 32F111  		ld	(sg_last_stroke),a
              		
