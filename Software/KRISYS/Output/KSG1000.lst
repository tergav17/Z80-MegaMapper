03D6: 3AEC0D  		ld	a,(res_argument)
03D9: D640    		sub	'A'-1
03DB: FE11    		cp	17
03DD: D24004  		jp	nc,99$
03E0: 32860E  		ld	(res_fcb_drive),a
03E3: 23      		inc	hl
03E4: 23      		inc	hl
              	
