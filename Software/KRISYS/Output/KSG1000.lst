0461: 7E      	0$:	ld	a,(hl)
0462: B7      		or	a
0463: CAC004  		jp	z,99$
0466: FE21    		cp	0x21
0468: D26F04  		jp	nc,1$
046B: 23      		inc	hl
046C: C36104  		jp	0$
              		
