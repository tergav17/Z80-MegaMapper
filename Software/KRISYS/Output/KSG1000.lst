              		; INI-class?
052C: 0F      		rrca
052D: DAF805  		jp	c,trap_io_inx
              		
              		; Left or right column
0530: 0F      		rrca
0531: DA5B05  		jp	c,0$
              		
              		; Left column
              		; B, D, H, or 0?
0534: 0F      		rrca
0535: DA4A05  		jp	c,1$
              		
              		; B or H?
0538: 0F      		rrca
0539: DA4305  		jp	c,2$
              		
              		; It's B
053C: CDE909  		call	in_handle
053F: 47      		ld	b,a
0540: C38505  		jp	90$
              		
              		; It's H	
0543: CDE909  	2$:	call	in_handle
0546: 67      		ld	h,a
0547: C38505  		jp	90$
              	
              		; D or 0?
054A: 0F      	1$:	rrca
054B: DA5505  		jp	c,3$
              	
              		; It's D
054E: CDE909  		call	in_handle
0551: 57      		ld	d,a
0552: C38505  		jp	90$
              		
              		; It's 0
0555: CDE909  	3$:	call	in_handle
0558: C38505  		jp	90$
              		
              		
              		; Right column
              		; C, E, L, or A?
055B: 0F      	0$:	rrca
055C: DA7105  		jp	c,4$
              		
              		; C or L?
055F: 0F      		rrca
0560: DA6A05  		jp	c,5$
              		
              		; It's C
0563: CDE909  		call	in_handle
0566: 4F      		ld	c,a
0567: C38505  		jp	90$
              	
              		; It's L
056A: CDE909  	5$:	call	in_handle
056D: 6F      		ld	l,a
056E: C38505  		jp	90$
              		
              		; E or A?
0571: 0F      	4$:	rrca
0572: DA7C05  		jp	c,6$
              		
              		; It's E
0575: CDE909  		call	in_handle
0578: 5F      		ld	e,a
