              		; CRLF
0393: 0E09    		ld	c,bdos_print
0395: 11D30A  		ld	de,str_crlf
0398: CD0500  		call	bdos
              		
              		; Detect if there is an argument
039B: 3AA50D  		ld	a,(res_argument)
039E: B7      		or	a
039F: C2AD03  		jp	nz,1$
              		
              		; No argument, error!
03A2: 0E09    		ld	c,bdos_print
03A4: 118209  		ld	de,str_arg_empty
03A7: CD0500  		call	bdos
03AA: C35C08  		jp	cpm_exit
              	
              		; Reset fields
03AD: AF      	1$:	xor	a
03AE: 213F0E  		ld	hl,res_fcb
03B1: 11400E  		ld	de,res_fcb+1
03B4: 012300  		ld	bc ,36-1
03B7: 77      		ld	(hl),a
03B8: EDB0    		ldir
              		
03BA: 3E20    		ld	a,0x20
03BC: 21400E  		ld	hl,res_fcb_name
03BF: 11410E  		ld	de,res_fcb_name+1
03C2: 010A00  		ld	bc ,11-1
03C5: 77      		ld	(hl),a
03C6: EDB0    		ldir
              	
              		; Is there a prefix?
03C8: 21A50D  		ld	hl,res_argument
03CB: 3AA60D  		ld	a,(res_argument+1)
03CE: FE3A    		cp	':'
03D0: C2E203  		jp	nz,2$
              		
              		; Set prefix
03D3: 3AA50D  		ld	a,(res_argument)
03D6: D640    		sub	'A'-1
03D8: FE11    		cp	17
03DA: D23D04  		jp	nc,99$
03DD: 323F0E  		ld	(res_fcb_drive),a
03E0: 23      		inc	hl
03E1: 23      		inc	hl
              	
              		; HL = Proper filename start
03E2: 0608    	2$:	ld	b,8
03E4: 11400E  		ld	de,res_fcb_name
              		
              		; Copy it over
03E7: 7E      	3$:	ld	a,(hl)
03E8: B7      		or	a
03E9: CA3D04  		jp	z,99$
03EC: FE2A    		cp	'*'
03EE: CAFE03  		jp	z,4$
03F1: FE2E    		cp	'.'
03F3: CA0504  		jp	z,5$
03F6: 12      		ld	(de),a
03F7: 13      		inc	de
03F8: 23      		inc	hl
03F9: 10EC    		djnz	3$
03FB: C30504  		jp	5$
              	
              		; Fill remains of FCB file name
03FE: 3E3F    	4$:	ld	a,'?'
0400: 12      		ld	(de),a
0401: 13      		inc	de
0402: 10FA    		djnz	4$
0404: 23      		inc	hl
              	
              		; We should either see a '.' or a null character
0405: 7E      	5$:	ld	a,(hl)
0406: B7      		or	a
0407: CA2E04  		jp	z,8$
040A: FE2E    		cp	'.'
040C: C23D04  		jp	nz,99$
040F: 23      		inc	hl
              		
              		; Fill in extension
0410: 0603    		ld	b,3
0412: 11480E  		ld	de,res_fcb_type
              		
