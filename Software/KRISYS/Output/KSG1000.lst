047F: 7E      	3$:	ld	a,(hl)
0480: B7      		or	a
0481: CABD04  		jp	z,99$
0484: FE21    		cp	0x21
0486: DA5E04  		jp	c,0$
0489: 23      		inc	hl
048A: C37F04  		jp	3$
              	
