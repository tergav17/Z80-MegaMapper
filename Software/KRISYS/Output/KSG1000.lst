03B0: AF      	1$:	xor	a
03B1: 21860E  		ld	hl,res_fcb
03B4: 11870E  		ld	de,res_fcb+1
03B7: 012300  		ld	bc ,36-1
03BA: 77      		ld	(hl),a
03BB: EDB0    		ldir
              		
03BD: 3E20    		ld	a,0x20
03BF: 21870E  		ld	hl,res_fcb_name
03C2: 11880E  		ld	de,res_fcb_name+1
03C5: 010A00  		ld	bc ,11-1
03C8: 77      		ld	(hl),a
03C9: EDB0    		ldir
              	
              		; Is there a prefix?
03CB: 21EC0D  		ld	hl,res_argument
03CE: 3AED0D  		ld	a,(res_argument+1)
03D1: FE3A    		cp	':'
03D3: C2E503  		jp	nz,2$
              		
