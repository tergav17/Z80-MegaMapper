              	; Initalize resources
              	;
              	; Returns nothing
              	; Uses: AF, BC, HL
02B5:         	res_init:
              		; Start by zero-terminating string
02B5: 218000  		ld	hl,cpm_command
02B8: 4E      		ld	c,(hl)
02B9: 0600    		ld	b,0
02BB: 09      		add	hl,bc
02BC: 23      		inc	hl
02BD: 70      		ld	(hl),b
              		
02BE: C9      		ret
              		
              	; Loads an open resource into a bankmap
              	; Bankmap will be zero-padded to fill requested number of records
              	; HL = Address of bankmap
              	; BC = Number of records (128 byte blocks) to load
              	;
              	; Returns nothing
              	; Uses: all
02BF:         	res_load:
              		; Set read flag
02BF: 3EFF    		ld	a,0xFF
02C1: 323A0E  		ld	(res_do_read),a
              	
              		; Save args
02C4: 223B0E  		ld	(res_bankmap),hl
02C7: ED43380E		ld	(res_sectors),bc
              	
              		; Virtual mode should be off while we do this
02CB: 3AA00D  		ld	a,(zmm_ctrl_state)
02CE: F5      		push	af
02CF: CD1002  		call	zmm_set_real
              		
              		; Do function call
02D2: CDE102  		call	0$
              		
              		; Reset bank 3
02D5: 3AA40D  		ld	a,(zmm_bnk3_state)
02D8: D333    		out	(zmm_bnk3),a
              		
              		; Restore register
02DA: F1      		pop	af
02DB: 32A00D  		ld	(zmm_ctrl_state),a
02DE: C3FF01  		jp	zmm_ctrl_set
              		
              		; Allocate a new bank
02E1: 1601    	0$:	ld	d,1
02E3: CD6501  		call	mem_alloc
02E6: 2A3B0E  		ld	hl,(res_bankmap)
02E9: 77      		ld	(hl),a
02EA: 23      		inc	hl
02EB: 223B0E  		ld	(res_bankmap),hl
              		
              		; Set the bank in slot 3
02EE: D333    		out	(zmm_bnk3),a
              		
              		; Set DMA address
02F0: 11B80D  		ld	de,res_buffer
02F3: 0E1A    		ld	c,bios_set_dma
02F5: CD0500  		call	bdos
              		
              		; Set pointer	
02F8: 2100C0  		ld	hl,zmm_top
02FB: 223D0E  		ld	(res_pointer),hl
              		
              		; We will be loading up to 128 records at a time here
02FE: 0680    		ld	b,128
              		
              		; Do read operation
0300: C5      	1$:	push	bc
0301: 113F0E  		ld	de,res_fcb
0304: 0E14    		ld	c,bdos_read
0306: 3A3A0E  		ld	a,(res_do_read)
0309: B7      		or	a
030A: C40500  		call	nz,bdos
              		
              		; Check file I/O result
030D: B7      		or	a
030E: C43703  		call	nz,50$
              		
              		; Go to virtual mode
0311: CD0502  		call	zmm_set_virt
              		
              		; Copy record to bank
0314: 21B80D  		ld	hl,res_buffer
0317: ED5B3D0E		ld	de,(res_pointer)
031B: 018000  		ld	bc,128
031E: EDB0    		ldir
0320: ED533D0E		ld	(res_pointer),de
              		
              		; Back to real mode
0324: CD1002  		call	zmm_set_real 
              		
              		; Get BC back to restore sector counter
0327: C1      		pop	bc
              		
              		; Decrement record count
0328: 2A380E  		ld	hl,(res_sectors)
032B: 2B      		dec	hl
032C: 22380E  		ld	(res_sectors),hl
032F: 7C      		ld	a,h
0330: B5      		or	l
0331: C8      		ret	z
              		
              		; Go get another sector
0332: 10CC    		djnz	1$
0334: C3E102  		jp	0$
              	
              		; Zero buffer and reset read flag
0337: AF      	50$:	xor	a
0338: 323A0E  		ld	(res_do_read),a
              		
033B: 21B80D  		ld	hl,res_buffer
033E: 11B90D  		ld	de,res_buffer+1
0341: 017F00  		ld	bc,128-1
0344: 77      		ld	(hl),a
0345: EDB0    		ldir
              	
0347: C9      		ret
              		
