03EA: 7E      	3$:	ld	a,(hl)
03EB: B7      		or	a
03EC: CA4004  		jp	z,99$
03EF: FE2A    		cp	'*'
03F1: CA0104  		jp	z,4$
03F4: FE2E    		cp	'.'
03F6: CA0804  		jp	z,5$
03F9: 12      		ld	(de),a
03FA: 13      		inc	de
03FB: 23      		inc	hl
03FC: 10EC    		djnz	3$
03FE: C30804  		jp	5$
              	
