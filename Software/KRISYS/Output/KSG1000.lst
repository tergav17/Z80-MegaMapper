034F: 21920E  		ld	hl,res_buffer
0352: ED5B170F		ld	de,(res_pointer)
0356: 018000  		ld	bc,128
0359: EDB0    		ldir
035B: ED53170F		ld	(res_pointer),de
              		
              		; Back to real mode
035F: CD4B02  		call	zmm_set_real 
              		
