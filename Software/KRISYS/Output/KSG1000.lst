              		; Save args
02FF: 22150F  		ld	(res_bankmap),hl
0302: ED43120F		ld	(res_sectors),bc
              	
              		; Virtual mode should be off while we do this
0306: 3A7A0E  		ld	a,(zmm_ctrl_state)
0309: F5      		push	af
030A: CD4B02  		call	zmm_set_real
              		
              		; Do function call
030D: CD1C03  		call	0$
              		
              		; Reset bank 3
0310: 3A7E0E  		ld	a,(zmm_bnk3_state)
0313: D333    		out	(zmm_bnk3),a
              		
