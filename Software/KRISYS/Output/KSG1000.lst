014C: 3A9303  		ld	a,(banks_free)
014F: CDDD02  		call	tohex
0152: ED531C03		ld	(str_mem_init_cnt),de
0156: 11F702  		ld	de,str_mem_init
0159: CDC502  		call	cpm_print
              		
              		; Do we actually have an acceptable amount of memory?
015C: 3A9303  		ld	a,(banks_free)
015F: 3D      		dec	a
0160: 3D      		dec	a
0161: FAA301  		jp	m,mem_empty
              		
              		; We do, return
0164: C9      		ret
              		
              	; Allocates a bank of memory
              	; Will produce an error if no banks are available,
              	; check (banks_free) to avoid
