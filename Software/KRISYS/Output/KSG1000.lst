              	;
              	;**************************************************************
              	;*
              	;*         S N 7 6 4 8 9   P S G   E M U L A T O R
              	;*
              	;*    Translates audio data meant for a SN76489 PSG into
              	;*    commands for the AY-3-8910.
              	;*
              	;*    This translation isn't perfect, but for most games
              	;*    it's "good enough".
              	;* 
              	;**************************************************************
              	
              	
              	; ----------------------------
              	; ********  Functions ********
              	; ----------------------------
              	
              	.area	_TEXT
              	
              	; Reset the PSG emulator
              	;
              	; Returns nothing
              	; Uses: AF, HL
0A12:         	snpsg_reset:
              	
              		; Reset registers
0A12: 210000  		ld	hl,0
0A15: 22E211  		ld	(snpsg_freq),hl
0A18: 22E411  		ld	(snpsg_freq+2),hl
0A1B: 22E611  		ld	(snpsg_freq+4),hl
              	
0A1E: 3E0F    		ld	a,0x0F
0A20: 32E811  		ld	(snpsg_atten),a
0A23: 32E911  		ld	(snpsg_atten+1),a
0A26: 32EA11  		ld	(snpsg_atten+2),a
0A29: 32EB11  		ld	(snpsg_atten+3),a
              	
0A2C: AF      		xor	a
0A2D: 32EC11  		ld	(snpsg_n_ctrl),a
              	
              		; Reset AY-3-8910
0A30: 3E07    		ld	a,7
0A32: D341    		out	(nabu_ay_latch),a
0A34: 3E7F    		ld	a,0b01111111
0A36: D340    		out	(nabu_ay_data),a
              	
0A38: C9      		ret
              		
              		
              		
              		
              	; Send a byte to the "SN76489"
              	; A = Byte to send
              	;
              	; Uses: AF
0A39:         	snpsg_send:
0A39: C5      		push	bc
0A3A: E5      		push 	hl
              		
              		; Save latest command byte
0A3B: 47      		ld	b,a
0A3C: 0E00    		ld	c,0
              		
              		; Check bit 7
0A3E: 07      		rlca
0A3F: D2870A  		jp	nc,30$
              		
              		; Get register offset
0A42: 07      		rlca
0A43: CB11    		rl	c
0A45: 07      		rlca
0A46: CB11    		rl	c
              		
              		; Frequency or attenuation?
0A48: 07      		rlca
0A49: D2610A  		jp	nc,10$
              		
              		; Attenuation
0A4C: 78      		ld	a,b
0A4D: E60F    		and	0b00001111
0A4F: 0600    		ld	b,0
0A51: 21E811  		ld	hl,snpsg_atten
0A54: 09      		add	hl,bc
              		
              		; Translate attenuation
0A55: E5      		push	hl
0A56: 4F      		ld	c,a
0A57: 212A0E  		ld	hl,snpsg_vol_tab
0A5A: 09      		add	hl,bc
0A5B: 7E      		ld	a,(hl)
0A5C: E1      		pop	hl
0A5D: 77      		ld	(hl),a
0A5E: C39F0A  		jp	80$
              		
              		; Frequency (maybe)
0A61: 3E03    	10$:	ld	a,0x03
0A63: B9      		cp	c
0A64: CA7E0A  		jp	z,20$
              	
              		; Frequency (low bits)
0A67: 78      		ld	a,b
0A68: 0600    		ld	b,0
0A6A: CB21    		sla	c
0A6C: 21E211  		ld	hl,snpsg_freq
0A6F: 09      		add	hl,bc
0A70: E60F    		and	0b00001111
0A72: 47      		ld	b,a
0A73: 7E      		ld	a,(hl)
0A74: E6F0    		and	0b11110000
0A76: B0      		or	b
0A77: 77      		ld	(hl),a
0A78: 22ED11  		ld	(snpsg_lastf),hl
0A7B: C39F0A  		jp	80$
              		
              		; Noise control
0A7E: 78      	20$:	ld	a,b
0A7F: E60F    		and	0b00001111
0A81: 32EC11  		ld	(snpsg_n_ctrl),a
0A84: C39F0A  		jp	80$
              		
              		; Frequency (high bits)
0A87: 78      	30$:	ld	a,b
0A88: 0600    		ld	b,0
0A8A: 07      		rlca
0A8B: 07      		rlca
0A8C: 07      		rlca
0A8D: CB10    		rl	b
0A8F: 07      		rlca
0A90: CB10    		rl	b
0A92: E6F0    		and	0b11110000
0A94: 4F      		ld	c,a
0A95: 2AED11  		ld	hl,(snpsg_lastf)
0A98: 3E0F    		ld	a,0b00001111
0A9A: A6      		and	(hl)
0A9B: B1      		or	c
0A9C: 77      		ld	(hl),a
0A9D: 23      		inc	hl
0A9E: 70      		ld	(hl),b
              		
              		; Update the state to the AY-3-8910
0A9F: 3E00    	80$:	ld	a,0
0AA1: 0E40    		ld	c,nabu_ay_data
0AA3: 21E211  		ld	hl,snpsg_freq
              		
              		; Set frequency
0AA6: D341    	81$:	out	(nabu_ay_latch),a
0AA8: EDA3    		outi
0AAA: 3C      		inc	a
0AAB: FE06    		cp	6
0AAD: C2A60A  		jp	nz,81$
              		
              		; Set channel mask
0AB0: 3E07    		ld	a,7
0AB2: D341    		out	(nabu_ay_latch),a
0AB4: 3E78    		ld	a,0b01111000
0AB6: D340    		out	(nabu_ay_data),a
              		
              		; Set amplitude
0AB8: 21E811  		ld	hl,snpsg_atten
0ABB: 0E08    		ld	c,8
0ABD: 0603    		ld	b,3
0ABF: 79      	82$:	ld	a,c
0AC0: D341    		out	(nabu_ay_latch),a
0AC2: 7E      		ld	a,(hl)
0AC3: FE0F    		cp	0x0F
0AC5: C2C90A  		jp	nz,83$
0AC8: AF      		xor	a
0AC9: D340    	83$:	out	(nabu_ay_data),a
0ACB: 0C      		inc	c
0ACC: 10F1    		djnz	82$
              		
0ACE: E1      	99$	pop	hl
0ACF: C1      		pop	bc
0AD0: C9      		ret
              	
